    <!--Barcelona Worksapce-->
    <Workspace id="Barcelona Hospital" description="All model elements and instances of the hospital in Barcelona">
        <Reader principalId="Barcelona-Hospital"/>
        <Writer principalId="Barcelona-Hospital"/>
