                </HumanTaskDefinition>
            </StageDefinition>
        
            <StageDefinition id="CaseEvaluation" description="Case Evaluation" isMandetory="true" isRepeatable="true">  
                <SentryDefinition>
                    <precondition processDefinitionId="CaseIdentification"/>
                </SentryDefinition>
                <HumanTaskDefinition id="Barthel" description="Barthel" ownerPath="Owner" isMandetory="true" isRepeatable="false" entityDefinitionId="Barthel" entityAttachPath="Barthel">
                    <TaskParamDefinition path="Barthel.barthel1" isReadOnly="false" isMandetory="true"/>
