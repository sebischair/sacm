            <StageDefinition id="WorkplanDefinition" description="Workplan Definition" isMandetory="true" isRepeatable="true">  
                <SentryDefinition>
                    <precondition processDefinitionId="CaseEvaluation"/>
                </SentryDefinition>
                <HumanTaskDefinition id="PhysicalActivityPrescription" description="Physical Activity Prescription" ownerPath="Owner" isMandetory="true" isRepeatable="false" entityDefinitionId="PhysicalActivity" entityAttachPath="PhysicalActivity">
