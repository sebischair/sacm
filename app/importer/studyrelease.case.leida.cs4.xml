   <!-- CASE DEFINITION -->				  
        <CaseDefinition id="LCS4_Leida" description="CS Carpe Diem" ownerPath="LCS4_Settings.CaseManager" clientPath="LCS4_Settings.Patient" entityDefinitionId="LCS4_CaseData" newEntityDefinitionId="LCS4_Settings" newEntityAttachPath="LCS4_Settings" 
		    notesDefaultValue=''									
            onCompleteHTTPHookURL="http://integration-producer:8081/v1/producer/point-to-point/sacm/case/terminate"
            onTerminateHTTPHookURL="http://integration-producer:8081/v1/producer/point-to-point/sacm/case/terminate"
            onDeleteHTTPHookURL="http://integration-producer:8081/v1/producer/point-to-point/sacm/case/terminate">	  
     	 
	    <!-- STAGEDEFINITION: CaseIdentification -->	  
            <StageDefinition id="LCS4_CaseIdentification" description="Case Identification" isMandatory="true" repeatable="ONCE" entityDefinitionId="LCS4_Identification" entityAttachPath="LCS4_Identification">
            
                <HumanTaskDefinition id="LCS4_SelectPatient" externalId="SelectPatient" description="Inclusion" ownerPath="LCS4_Settings.CaseManager" isMandatory="true" repeatable="ONCE">
                    <HttpHookDefinition on="COMPLETE" url="http://integration-producer:8081/v1/producer/point-to-point/sacm/case/patient/inclusion" method="POST" failureMessage="Could not COMPLETE SelectPatient on Self-Management System!"/>
                    <TaskParamDefinition path="LCS4_Settings.Patient" isReadOnly="false" isMandatory="true"/>                
                    <TaskParamDefinition path="LCS4_Settings.CaseManager" isReadOnly="false" isMandatory="true"/> 
                </HumanTaskDefinition>								
				
				<HumanTaskDefinition id="LCS4_LifeStyleHabits" description="Life Style Habits" ownerPath="LCS4_Settings.CaseManager" isMandatory="true" repeatable="ONCE"  entityDefinitionId="LCS4_LifeStyleHabits" entityAttachPath="LCS4_Identification.LCS4_LifeStyleHabits">
					<SentryDefinition>
                        <precondition processDefinitionId="LCS4_SelectPatient"/>
					</SentryDefinition>
					<TaskParamDefinition path="LCS4_Identification.LCS4_LifeStyleHabits.lsh1" isReadOnly="false" isMandatory="true"/>
					<TaskParamDefinition path="LCS4_Identification.LCS4_LifeStyleHabits.lsh2" isReadOnly="false" isMandatory="true"/>
					<TaskParamDefinition path="LCS4_Identification.LCS4_LifeStyleHabits.lsh3" isReadOnly="false" isMandatory="true"/>
					<TaskParamDefinition path="LCS4_Identification.LCS4_LifeStyleHabits.lsh4" isReadOnly="false" isMandatory="false"/>
					<TaskParamDefinition path="LCS4_Identification.LCS4_LifeStyleHabits.lsh5" isReadOnly="false" isMandatory="true"/>
					<TaskParamDefinition path="LCS4_Identification.LCS4_LifeStyleHabits.lsh6" isReadOnly="false" isMandatory="false"/>
					<TaskParamDefinition path="LCS4_Identification.LCS4_LifeStyleHabits.lsh7" isReadOnly="false" isMandatory="true"/>
					<TaskParamDefinition path="LCS4_Identification.LCS4_LifeStyleHabits.lsh8" isReadOnly="false" isMandatory="false"/>
				</HumanTaskDefinition>			 								
			</StageDefinition>				
			
            <!-- STAGEDEFINITION: Workplan -->    
            <StageDefinition id="LCS4_Workplan" description="Workplan" isMandatory="true" repeatable="ONCE" entityDefinitionId="LCS4_Workplan" entityAttachPath="LCS4_Workplan">			
                <SentryDefinition>
                    <precondition processDefinitionId="LCS4_CaseIdentification"/>
                </SentryDefinition> 		
				
                <DualTaskDefinition id="LCS4_MonitoringPrescriptionWeightAHWP" externalId="MonitoringPrescription"
				description="Monitoring Prescription - Weight"
				ownerPath="LCS4_Settings.CaseManager" dueDatePath="LCS4_Settings.WorkplanDueDate"  isMandatory="false" repeatable="PARALLEL" activation="MANUAL" entityDefinitionId="LCS4_MonitoringPrescriptionWeight" entityAttachPath="LCS4_Workplan.LCS4_MonitoringPrescriptionWeightAH">
                    <HttpHookDefinition on="ACTIVATEAUTOMATEDPART" url="http://integration-producer:8081/v1/producer/point-to-point/sacm/prescription" method="POST" failureMessage="Could not create prescription on Self-Management System!"/>
                    <HttpHookDefinition on="TERMINATE" url="http://integration-producer:8081/v1/producer/point-to-point/sacm/prescription/terminate" method="POST" failureMessage="Could not terminate prescription on Self-Management System!"/>
                    <TaskParamDefinition part="HUMAN" path="LCS4_Workplan.LCS4_MonitoringPrescriptionWeightAH.type" isReadOnly="false" isMandatory="true"/>
					
					<TaskParamDefinition part="HUMAN" path="LCS4_Workplan.LCS4_MonitoringPrescriptionWeightAH.alertmin" isReadOnly="false" isMandatory="false"/>
                    <TaskParamDefinition part="HUMAN" path="LCS4_Workplan.LCS4_MonitoringPrescriptionWeightAH.alertmax" isReadOnly="false" isMandatory="false"/>
					<TaskParamDefinition part="HUMAN" path="LCS4_Workplan.LCS4_MonitoringPrescriptionWeightAH.alertfreq" isReadOnly="false" isMandatory="true"/>
                    <TaskParamDefinition part="HUMAN" path="LCS4_Workplan.LCS4_MonitoringPrescriptionWeightAH.alertfrequnit" isReadOnly="false" isMandatory="true"/>
					
                    <TaskParamDefinition part="HUMAN" path="LCS4_Workplan.LCS4_MonitoringPrescriptionWeightAH.freq" isReadOnly="false" isMandatory="true"/>
                    <TaskParamDefinition part="HUMAN" path="LCS4_Workplan.LCS4_MonitoringPrescriptionWeightAH.frequnit" isReadOnly="false" isMandatory="true"/>   
                    <TaskParamDefinition part="HUMAN" path="LCS4_Workplan.LCS4_MonitoringPrescriptionWeightAH.maxdelay" isReadOnly="false" isMandatory="true"/>  	
                    <TaskParamDefinition part="HUMAN" path="LCS4_Workplan.LCS4_MonitoringPrescriptionWeightAH.startdate" isReadOnly="false" isMandatory="true"/>  
                    <TaskParamDefinition part="HUMAN" path="LCS4_Workplan.LCS4_MonitoringPrescriptionWeightAH.enddate" isReadOnly="false" isMandatory="true"/>  
                    <TaskParamDefinition part="HUMAN" path="LCS4_Workplan.LCS4_MonitoringPrescriptionWeightAH.timeslot" isReadOnly="false" isMandatory="true"/> 
                    <TaskParamDefinition part="HUMAN" path="LCS4_Workplan.LCS4_MonitoringPrescriptionWeightAH.comments" isReadOnly="false" isMandatory="false"/>                    
                    <TaskParamDefinition part="AUTOMATED" path="LCS4_Workplan.LCS4_MonitoringPrescriptionWeightAH.measurement" isReadOnly="false" isMandatory="true"/>
                </DualTaskDefinition>							
				                				
                <DualTaskDefinition id="LCS4_PhysicalActivityAHWP" externalId="PhysicalActivity"
				description="Monitoring Prescription - Physical Activity"
				ownerPath="LCS4_Settings.CaseManager" dueDatePath="LCS4_Settings.WorkplanDueDate"  isMandatory="false" repeatable="PARALLEL" activation="MANUAL" entityDefinitionId="LCS4_PhysicalActivity" entityAttachPath="LCS4_Workplan.LCS4_PhysicalActivityAH">
                    <HttpHookDefinition on="ACTIVATEAUTOMATEDPART" url="http://integration-producer:8081/v1/producer/point-to-point/sacm/prescription" method="POST" failureMessage="Could not create prescription on Self-Management System!"/>
                    <HttpHookDefinition on="TERMINATE" url="http://integration-producer:8081/v1/producer/point-to-point/sacm/prescription/terminate" method="POST" failureMessage="Could not terminate prescription on Self-Management System!"/>
                    <TaskParamDefinition part="HUMAN" path="LCS4_Workplan.LCS4_PhysicalActivityAH.startdate" isReadOnly="false" isMandatory="true"/>
                    <TaskParamDefinition part="HUMAN" path="LCS4_Workplan.LCS4_PhysicalActivityAH.enddate" isReadOnly="false" isMandatory="true"/>
                    <TaskParamDefinition part="HUMAN" path="LCS4_Workplan.LCS4_PhysicalActivityAH.dailysteps" isReadOnly="false" isMandatory="true"/>
                    <TaskParamDefinition part="HUMAN" path="LCS4_Workplan.LCS4_PhysicalActivityAH.dailylowlevel" isReadOnly="false" isMandatory="true"/>
                    <TaskParamDefinition part="HUMAN" path="LCS4_Workplan.LCS4_PhysicalActivityAH.dailymediumlevel" isReadOnly="false" isMandatory="true"/>
					<TaskParamDefinition part="HUMAN" path="LCS4_Workplan.LCS4_PhysicalActivityAH.dailyhighlevel" isReadOnly="false" isMandatory="true"/>					
					<TaskParamDefinition part="HUMAN" path="LCS4_Workplan.LCS4_PhysicalActivityAH.handedness" isReadOnly="false" isMandatory="true"/>
					<TaskParamDefinition part="HUMAN" path="LCS4_Workplan.LCS4_PhysicalActivityAH.wristplacement" isReadOnly="false" isMandatory="true"/>
                    <TaskParamDefinition part="AUTOMATED" path="LCS4_Workplan.LCS4_PhysicalActivityAH.measurement" isReadOnly="false" isMandatory="true"/>
                </DualTaskDefinition>			
				
                <DualTaskDefinition id="LCS4_PatientQuestionaireAHWP" externalId="PatientQuestionaire" 
				description="Prescription - Patient Questionaire"
                                    dynamicDescriptionPath="LCS4_Workplan.LCS4_PatientQuestionaireAH.type"
				ownerPath="LCS4_Settings.CaseManager" dueDatePath="LCS4_Settings.WorkplanDueDate"  isMandatory="false" repeatable="PARALLEL" activation="MANUAL" 
				entityDefinitionId="LCS4_PatientQuestionaire" entityAttachPath="LCS4_Workplan.LCS4_PatientQuestionaireAH">
                    <HttpHookDefinition on="ACTIVATEAUTOMATEDPART" url="http://integration-producer:8081/v1/producer/point-to-point/sacm/prescription/questionnaires" method="POST" failureMessage="Could not create prescription on Self-Management System!"/>
