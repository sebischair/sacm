                    <TaskParamDefinition part="HUMAN" path="LCS4_Workplan.LCS4_PatientQuestionaireAH.type" isReadOnly="false" isMandatory="true"/>
                    <TaskParamDefinition part="HUMAN" path="LCS4_Workplan.LCS4_PatientQuestionaireAH.startdate" isReadOnly="false" isMandatory="true"/>
                    <TaskParamDefinition part="HUMAN" path="LCS4_Workplan.LCS4_PatientQuestionaireAH.enddate" isReadOnly="false" isMandatory="true"/>
                    <TaskParamDefinition part="HUMAN" path="LCS4_Workplan.LCS4_PatientQuestionaireAH.freq" isReadOnly="false" isMandatory="true"/>
                    <TaskParamDefinition part="HUMAN" path="LCS4_Workplan.LCS4_PatientQuestionaireAH.frequnit" isReadOnly="false" isMandatory="true"/>   
                    <TaskParamDefinition part="HUMAN" path="LCS4_Workplan.LCS4_PatientQuestionaireAH.maxdelay" isReadOnly="false" isMandatory="true"/> 
                    <TaskParamDefinition part="HUMAN" path="LCS4_Workplan.LCS4_PatientQuestionaireAH.timeslot" isReadOnly="false" isMandatory="true"/> 
                    <TaskParamDefinition part="HUMAN" path="LCS4_Workplan.LCS4_PatientQuestionaireAH.repetitionsperday" isReadOnly="false" isMandatory="true"/>                    
                    <TaskParamDefinition part="AUTOMATED" path="LCS4_Workplan.LCS4_PatientQuestionaireAH.questionaires" isReadOnly="false" isMandatory="true"/>
                </DualTaskDefinition> 	
												
			</StageDefinition>			
		  	 
            <StageDefinition id="LCS4_Discharge" description="Discharge" isMandatory="false" repeatable="ONCE" entityDefinitionId="LCS4_Discharge" entityAttachPath="LCS4_Discharge">
			
                <HumanTaskDefinition id="LCS4_DischargeTestHT" description="Discharge Form"
				ownerPath="LCS4_Settings.CaseManager" isMandatory="true" repeatable="ONCE" entityDefinitionId="LCS4_DischargeTest" entityAttachPath="LCS4_Discharge.LCS4_DischargeTest">									
                    <TaskParamDefinition part="HUMAN" path="LCS4_Discharge.LCS4_DischargeTest.comment" isReadOnly="false" isMandatory="true"/>
				</HumanTaskDefinition>		
				
			</StageDefinition> 									 
		</CaseDefinition>	
