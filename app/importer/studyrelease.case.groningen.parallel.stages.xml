                    <TaskParamDefinition part="HUMAN" path="GCS2PS_PhysicalActivity.phactp1" isReadOnly="false" isMandatory="true"/>
                    <TaskParamDefinition part="HUMAN" path="GCS2PS_PhysicalActivity.phactp2" isReadOnly="false" isMandatory="true"/>
                    <TaskParamDefinition part="HUMAN" path="GCS2PS_PhysicalActivity.phactp3" isReadOnly="false" isMandatory="true"/>
                    <TaskParamDefinition part="HUMAN" path="GCS2PS_PhysicalActivity.phactp4" isReadOnly="false" isMandatory="true"/>
                    <TaskParamDefinition part="HUMAN" path="GCS2PS_PhysicalActivity.phactp5" isReadOnly="false" isMandatory="true"/>
                    <TaskParamDefinition part="HUMAN" path="GCS2PS_PhysicalActivity.phactp6" isReadOnly="false" isMandatory="true"/>
                    <TaskParamDefinition part="HUMAN" path="GCS2PS_PhysicalActivity.phactp7" isReadOnly="false" isMandatory="true"/>                    
                    <TaskParamDefinition part="AUTOMATED" path="GCS2PS_PhysicalActivity.phactpx" isReadOnly="false" isMandatory="true"/>
                </DualTaskDefinition>

